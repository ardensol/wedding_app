<h1>Vendor Leads</h1>

<table>
  <thead>
    <tr>
      <th>Quote type</th>
      <th>Wedding style</th>
      <th>Budget</th>
      <th>Comments</th>
      <th>Event date</th>
      <th>Event time</th>
      <th>Zip code</th>
      <th>Solicitation type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.quote_type %></td>
        <td><%= quote.wedding_style %></td>
        <td><%= quote.budget %></td>
        <td><%= quote.comments %></td>
        <td><%= quote.event_date %></td>
        <td><%= quote.event_time %></td>
        <td><%= quote.zip_code %></td>
        <td><%= quote.solicitation_type %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
